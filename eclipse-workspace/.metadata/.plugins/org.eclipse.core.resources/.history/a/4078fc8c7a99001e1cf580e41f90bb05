package com.mire.biz.tv;

public class BeanFactory {
	public Object setBean(String beanName) {
		Object obj = null;
		switch (beanName) {
		case "samsung":
			obj = new SamsungTV();
			break;
		case "lg":
			obj = new LgTV();
			break;
		case "google":
			
			break;
		default:
		}
		
		return obj;
	}
}
