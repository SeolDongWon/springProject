package com.mire.biz.tv;

public class LgTV {
	public void powerOn() {
		System.out.println("SamsunTV power on");
	}
	public void powerOff() {
		System.out.println("SamsunTV power off");
	}
	public void volumeUp() {
		System.out.println("SamsunTV volume up");
	}
	public void volumeDwon() {
		System.out.println("SamsunTV volume down");
	}

}
